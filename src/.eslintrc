{
    "extends": ["airbnb","prettier"],
    "env": {
        "browser": true
    },
    "praser": "label-eslint",
    "rules":{
        "no-plusplus": [0],
        "react/jsx.filename-extinsion": [1, {"extinsions": [".js",".jsx"]}],
        "react/prop-types": [0],
        "react/jsx-props-no-spreading": [0],
        "import/prefer-default-export": [0],
        "react/no-array-index-key": [0],
        "arrow-body-style": [0],
        "prefer-arrow-callback": [0]
    }
}